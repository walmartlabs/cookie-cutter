{
    "name": "@examples/grpc-chat",
    "private": true,
    "version": "0.0.0",
    "license": "Apache-2.0",
    "dependencies": {
        "@walmartlabs/cookie-cutter-core": "^1.4",
        "@walmartlabs/cookie-cutter-grpc": "^1.4",
        "inquirer": "8.1.0"
    },
    "devDependencies": {
        "protobufjs": "6.9.0"
    },
    "scripts": {
        "prebuild": "pbjs -t static-module -w commonjs -o ./src/chat.js chat.proto && pbts -o ./src/chat.d.ts ./src/chat.js",
